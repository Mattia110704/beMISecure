<p id="notice"><%= notice %></p>

<h1>Parkingevents</h1>

<table>
  <thead>
    <tr>
      <th>Parkingrequest</th>
      <th>Datetime</th>
      <th>Parkallowed</th>
      <th>Datetime</th>
      <th>Parkcompleted</th>
      <th>Datetime</th>
      <th>Getbikerequest</th>
      <th>Getbikecompleted</th>
      <th>Datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkingevents.each do |parkingevent| %>
      <tr>
        <td><%= parkingevent.parkingrequest %></td>
        <td><%= parkingevent.datetime %></td>
        <td><%= parkingevent.parkallowed %></td>
        <td><%= parkingevent.datetime %></td>
        <td><%= parkingevent.parkcompleted %></td>
        <td><%= parkingevent.datetime %></td>
        <td><%= parkingevent.getbikerequest %></td>
        <td><%= parkingevent.getbikecompleted %></td>
        <td><%= parkingevent.datetime %></td>
        <td><%= link_to 'Show', parkingevent %></td>
        <td><%= link_to 'Edit', edit_parkingevent_path(parkingevent) %></td>
        <td><%= link_to 'Destroy', parkingevent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

